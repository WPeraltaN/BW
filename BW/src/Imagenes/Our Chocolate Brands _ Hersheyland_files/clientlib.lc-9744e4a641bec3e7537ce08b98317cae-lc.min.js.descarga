document.addEventListener("DOMContentLoaded", function() {
		setTimeout(function() {
			var slides = document.querySelectorAll(".card-item.tns-item");
			for (var i = 0; i < slides.length; i++) {
				let parentSlideHeight = slides[i].parentElement.offsetHeight;
				slides[i].style.cssText = "height: " + parentSlideHeight + "px;";
			}
		}, 300);
});